{% extends 'base.html.twig' %}

{% block title %}Tuteurs{% endblock %}

{% block body %}
    <h1>Tuteurs</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tuteur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ tuteur.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ tuteur.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ tuteur.adresse }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ tuteur.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ tuteur.email }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ tuteur.createdAt ? tuteur.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Created_by</th>
                <td>{{ tuteur.createdBy }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_tuteurs_index') }}">back to list</a>

    <a href="{{ path('app_tuteurs_edit', {'id': tuteur.id}) }}">edit</a>

    {{ include('tuteurs/_delete_form.html.twig') }}
{% endblock %}
